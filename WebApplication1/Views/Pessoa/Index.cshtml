<!-- Script para Gerar o formulario de Pessoas -->
<script>
    function gerarPessoaJs() {
        console.log("gerarPessoaJs");
        debugger;

        //dados gerados pelo response
        var geraPonto = $("#geraPonto").prop("checked") == true ? "S" : "N";
        var sexo = $("#geraSexo").prop("checked") == true ? "H" : "M"; //validação ternario onde valido em apenas uma linha toda a instrução
        var gerarIdade = document.getElementById("Seleciona_Idade").value
        var uf = document.getElementById("cmbEstado").value;
        var cidade = $('select#cmbCidades option:selected').text();

        var url = '/Pessoa/GerarPessoa'
        $.post(url, { sexo, geraPonto, gerarIdade, uf, cidade }, function (response) {
            debugger;
            console.log(response);
            //campos que geram os dados onde #nome =  ID do campo e "val(response.Nome)" = Resposta do GET

            //$("#ID do JS").val(response.Nome_na_classe_model);
            $("#nome").val(response.Nome);
            $("#idade").val(response.Idade);
            $("#sexo").val(response.Sexo);
            $("#cpf").val(response.Cpf);
            $("#rg").val(response.Rg);
            $("#endereco").val(response.Endereco);
            $("#numero").val(response.Numero);
            $("#bairro").val(response.Bairro);
            $("#estado").val(response.Estado);
            $("#cidade").val(response.Cidade);
            $("#cep").val(response.Cep);
            $("#telefone_fixo").val(response.Telefone_fixo);
            $("#celular").val(response.Celular);
            $("#mae").val(response.Mae);
            $("#pai").val(response.Pai);
            $("#dataNasc").val(response.Data_nasc);
            $("#email").val(response.Email);
            $("#senha").val(response.Senha);
            $("#altura").val(response.Altura);
            $("#peso").val(response.Peso);
            $("#tipoSanguinio").val(response.Tipo_sanguineo);
            $("#cor").val(response.Cor);
            $("#signo").val(response.Signo);

        })
    }
</script>

<script>
    function BuscarCidades() {
        var uf = document.getElementById("cmbEstado").value;

        var url = '/Pessoa/BuscarCidades'// minha controler/ação
        $.post(url, { ufSelecionado: uf }, function (cidadesRetornadas) {

            var cbmCidades = $('#cmbCidades');
            cbmCidades.empty();
            var option = "";//limpa os campos de "<option>" para adicionar novos dados

            $.each(cidadesRetornadas, function (indice, item) {
                option = $("<option>", { "value": indice }).text(item);
                cbmCidades.append(option);
            });
        })
    }
</script>

